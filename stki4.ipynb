        "# Create a label encoding dictionary\n",
        "label_encoding = {\n",
        "    'positif': 2,\n",
        "    'netral': 1,\n",
        "    'negatif': 0\n",
        "}\n",
        "\n",
        "# Apply the encoding to the 'Label' column\n",
        "df['Label'] = df['Label'].str.strip(';').map(label_encoding)\n",
